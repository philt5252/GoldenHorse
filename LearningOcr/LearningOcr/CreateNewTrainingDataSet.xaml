<Window x:Class="LearningOcr.CreateNewTrainingDataSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CreateNewTrainingDataSet" Height="130" Width="292">
    <Grid>
        <TextBlock Focusable="False" Height="23" HorizontalAlignment="Left" Margin="12,11,0,0" Name="textBlock1" Text="Training Data Name" VerticalAlignment="Top" />
        <TextBox Focusable="True" Height="23" HorizontalAlignment="Left" Margin="12,29,0,0" Name="textBox1" VerticalAlignment="Top" Width="250" />
        <Button Content="Create" Height="23" HorizontalAlignment="Left" Margin="187,56,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" IsDefault="True" />
        <CheckBox Content="Load Default Characters" Height="16" HorizontalAlignment="Left" Margin="12,63,0,0" Name="checkBox1" VerticalAlignment="Top" IsChecked="True" />
    </Grid>
</Window>
